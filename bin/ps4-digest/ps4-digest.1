.Xr ps4-fetch 1 ,
.Xr ps4-install 1 ,
.Xr ps4-pkgdb 1 ,
.Xr ps4-query 1 ,
.Xr ps4-reconfigure 1 ,
.Xr ps4-remove 1 ,
.Xr ps4-rindex 1 ,
.Xr ps4-uchroot 1 ,
.Xr ps4-uunshare 1 ,
